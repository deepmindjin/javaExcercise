public class MakeAnswer {
	
	public static int[] make(){
		int randomNum = 0;
		int[] answers = new int[4];
		for(int i=0;i<4;i++){
			randomNum = (int)(Math.random()*9);
			if(i>0){
				for(int j=0;j<i;j++){
					if(answers[j]==randomNum){
						i--;
					}
				}
			}
			answers[i] = randomNum;
		}
		return answers;
	}
}
