
public class StrategyFactoryMethodMain {

	public static void main(String[] args) {
		Context context = new Context();
		StrategyInterface sfm1 = StrategyFactoryMethod.newInstance("Punch");
		context.runContext(sfm1);
		StrategyInterface sfm2 = StrategyFactoryMethod.newInstance("Kick");
		context.runContext(sfm2);
		StrategyInterface sfm3 = StrategyFactoryMethod.newInstance("Run");
		context.runContext(sfm3);
		
	}

}
