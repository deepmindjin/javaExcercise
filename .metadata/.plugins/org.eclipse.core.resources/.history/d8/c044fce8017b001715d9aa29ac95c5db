
public class LottoMain {

	public static void main(String[] args) {
		MakeNum lotteryMachine = new MakeNum();
		int[] pickedNums = lotteryMachine.getLotteryArray();
		System.out.println("당첨 번호는/n");
		for(int i=0;i<6;i++){
			int pickedNum = pickedNums[i];
			System.out.println(pickedNum + "번 ");	
		}
		System.out.println("입니다.");
	}

}
