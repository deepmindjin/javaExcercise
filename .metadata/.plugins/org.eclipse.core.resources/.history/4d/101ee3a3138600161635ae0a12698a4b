package com.Bbsmaking;

public class ㅇㅁㄴㅇㄻㄴㅇㄹ {
	boolean isPangram(String sentence) {
	    Boolean result = false;
	    Boolean singleNumValid = false;
	    String toUpper;
	    Integer[] upperNums = new Integer[36];
	    
	    for(int i = 65; i<=90;i++){
	        for(int j = 0;j<36;j++){
	            upperNums[j] = i;
	        }
	    }

	    toUpper = sentence.toUpperCase();
	    
	    for(int i=0;i<toUpper.length();i++){
	        singleNumValid = false;
	        int testChar = (int)toUpper.charAt(i);
	        if((65<=testChar)&&(testChar<=90)){
	            singleNumValid = true;
	        }
	        if(!singleNumValid){
	            return result;
	        }
	    }
	    result = true;
	    return result;
	}

}
