
public abstract class Car {
	
	public abstract void setDestination(String from, String to);
	
	
	private String name;
	private String to;
	private String from;
	
	public Car(String name){
		this.name = name;
	} 
	
	public void drive(){
		System.out.println(
				name+"을 몰고 "
				+from + "부터 "
				+to+"까지 이동한다");
	}
	
}
