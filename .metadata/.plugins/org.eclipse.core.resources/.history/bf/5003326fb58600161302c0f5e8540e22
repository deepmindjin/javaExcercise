
public class LambdaMain {

	public static void main(String[] args) {
		
		Sum sum = new Sum(){

			@Override
			public void absRun() {
				run();
				
			}
			
		};
		
		sum.absRun();

		Sum sum1 = new Sum(){

			@Override
			public void absRun() {
				System.out.println("Lambda");
			}
		};
		
		sum1.absRun();
	}

}